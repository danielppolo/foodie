<div class="navbar">
  <!-- Right Navigation -->
  <div class="navbar-right ">
    <% if user_signed_in? %>
      <!-- Links when logged in -->
      <%= link_to icon('fas', 'user'), user_path(current_user), class: "navbar-item navbar-link" %>
    <% else %>
      <!-- Login link (when logged out) -->
      <%= link_to t(".sign_in", default: "Enter"), new_user_session_path, class: "navbar-item navbar-wagon-link" %>
     <!--  %= link_to t(".sign_up", default: "Signup"), new_user_registration_path, class: "navbar-item navbar-wagon-link" %> -->
    <% end %>
  </div>
</div>
